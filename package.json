{
  "name": "workers-sdk-ci-analyzer",
  "version": "1.0.0",
  "description": "A dashboard for analyzing CI health and test flakiness in the cloudflare/workers-sdk repository",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "preview": "npm run build && vite preview",
    "deploy": "npm run build && wrangler deploy",
    "deploy:prod": "npm run build && wrangler deploy --env production",
    "typegen": "wrangler types",
    "typecheck": "tsc -p app/tsconfig.json && tsc -p workers/tsconfig.json",
    "trigger-cron": "wrangler deploy && curl -X POST https://workers-sdk-ci-analyzer.YOUR_SUBDOMAIN.workers.dev/api/refresh"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "ci",
    "dashboard",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-router/dev": "^7.9.6",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "isbot": "^5.1.32",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.9.6"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.19.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.9.3",
    "vite": "^6.1.0",
    "wrangler": "^4.57.0"
  }
}
